{
  "uid": "healthcare-kappa-poc",
  "title": "Healthcare Kappa POC",
  "tags": ["healthcare", "kappa", "spring-boot"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Outbox lag (seconds)",
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "platform_outbox_lag_seconds",
          "legendFormat": "lag",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Outbox events per second",
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rate(platform_outbox_events_processed_total[1m])",
          "legendFormat": "events/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Active SSE subscriptions",
      "gridPos": { "h": 7, "w": 8, "x": 16, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "platform_dashboard_sse_subscriptions",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "graphMode": "none",
        "colorMode": "value",
        "justifyMode": "auto"
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "JVM heap used",
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 7 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{area=\"heap\"}",
          "legendFormat": "heap used",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Heap usage %",
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 7 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "100 * (jvm_memory_used_bytes{area=\"heap\"} / jvm_memory_max_bytes{area=\"heap\"})",
          "legendFormat": "heap %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "hidden", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "CPU usage",
      "gridPos": { "h": 7, "w": 8, "x": 16, "y": 7 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "process_cpu_usage",
          "legendFormat": "process_cpu_usage",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "GC pause (avg & p95, 5m)",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rate(jvm_gc_pause_seconds_sum[5m]) / rate(jvm_gc_pause_seconds_count[5m])",
          "legendFormat": "avg pause",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(jvm_gc_pause_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95 pause",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "GC frequency (events/s, 5m)",
      "gridPos": { "h": 7, "w": 6, "x": 12, "y": 14 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rate(jvm_gc_pause_seconds_count[5m])",
          "legendFormat": "GC events/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "HTTP latency p95 (all)",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 21 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95 latency",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "HTTP latency p95 by endpoint",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 28 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{uri!~\"/actuator.*\"}[5m])) by (uri, le))",
          "legendFormat": "{{uri}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "Hikari connections (active/idle/max)",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 35 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "hikaricp_connections_active{pool=\"HikariPool-1\"}",
          "legendFormat": "active",
          "refId": "A"
        },
        {
          "expr": "hikaricp_connections_idle{pool=\"HikariPool-1\"}",
          "legendFormat": "idle",
          "refId": "B"
        },
        {
          "expr": "hikaricp_connections_max{pool=\"HikariPool-1\"}",
          "legendFormat": "max",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 13,
      "type": "timeseries",
      "title": "Hikari pending connections",
      "gridPos": { "h": 7, "w": 6, "x": 12, "y": 35 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "hikaricp_connections_pending{pool=\"HikariPool-1\"}",
          "legendFormat": "pending",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "hidden", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 14,
      "type": "timeseries",
      "title": "Hikari timeouts (5m rate)",
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 42 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rate(hikaricp_connections_timeout_total{pool=\"HikariPool-1\"}[5m])",
          "legendFormat": "timeouts/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "HTTP error rate (5xx %)",
      "gridPos": { "h": 7, "w": 6, "x": 12, "y": 21 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "100 * (sum(rate(http_server_requests_seconds_count{status=~\"5..\"}[5m])) / sum(rate(http_server_requests_seconds_count[5m])))",
          "legendFormat": "5xx %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "hidden", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "label": "Prometheus",
        "query": "prometheus",
        "current": {},
        "hide": 0,
        "refresh": 1
      }
    ]
  }
}
